<% if @snmp_trap_addr.is_a?(Array) and  @snmp_trap_addr.count > 0 -%>
snmpTrapdAddr <%= @snmp_trap_addr.join(',') %>
<% end -%>
authCommunity   log,execute,net <%= @ro_community %>
disableAuthorization <%= @disable_authorization %>
doNotLogTraps <%= @do_not_log_traps %>
<% @trap_handlers.each do |handler| -%>
traphandle <%= handler %>
<% end %>
<% @forwards.each do |forward| -%>
forward <%= forward %>
<% end %>
